{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "data-factory-copying-data"
		},
		"AzureDataLakeStorage1_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'AzureDataLakeStorage1'"
		},
		"AzureSynapseAnalytics1_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'AzureSynapseAnalytics1'"
		},
		"Onpremlinkedservice_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'Onpremlinkedservice'"
		},
		"cloudlinkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'cloudlinkedservice'"
		},
		"targetdblinked service_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'targetdblinked service'"
		},
		"AzureDataLakeStorage1_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sdproject8source.dfs.core.windows.net/"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "dbtargetsynapse.sql.azuresynapse.net"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "targentdata"
		},
		"AzureSynapseAnalytics1_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Ramon"
		},
		"Onpremlinkedservice_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "RAMON\\SQLEXPRESS"
		},
		"Onpremlinkedservice_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorks2008R2"
		},
		"Onpremlinkedservice_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa"
		},
		"cloudlinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sdbronzelayerrawdata.dfs.core.windows.net/"
		},
		"targetdblinked service_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "project8server.database.windows.net"
		},
		"targetdblinked service_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "project8database"
		},
		"targetdblinked service_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Ramon"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "data-factory-copying-data_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"AzureDataLakeStorage1_accountKey": {
						"value": "[parameters('AzureDataLakeStorage1_accountKey')]"
					},
					"AzureSynapseAnalytics1_password": {
						"value": "[parameters('AzureSynapseAnalytics1_password')]"
					},
					"cloudlinkedservice_accountKey": {
						"value": "[parameters('cloudlinkedservice_accountKey')]"
					},
					"targetdblinked service_password": {
						"value": "[parameters('targetdblinked service_password')]"
					},
					"Onpremlinkedservice_password": {
						"value": "[parameters('Onpremlinkedservice_password')]"
					},
					"AzureDataLakeStorage1_properties_typeProperties_url": {
						"value": "[parameters('AzureDataLakeStorage1_properties_typeProperties_url')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_server": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_server')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_database": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_database')]"
					},
					"AzureSynapseAnalytics1_properties_typeProperties_userName": {
						"value": "[parameters('AzureSynapseAnalytics1_properties_typeProperties_userName')]"
					},
					"cloudlinkedservice_properties_typeProperties_url": {
						"value": "[parameters('cloudlinkedservice_properties_typeProperties_url')]"
					},
					"targetdblinked service_properties_typeProperties_server": {
						"value": "[parameters('targetdblinked service_properties_typeProperties_server')]"
					},
					"targetdblinked service_properties_typeProperties_database": {
						"value": "[parameters('targetdblinked service_properties_typeProperties_database')]"
					},
					"targetdblinked service_properties_typeProperties_userName": {
						"value": "[parameters('targetdblinked service_properties_typeProperties_userName')]"
					},
					"Onpremlinkedservice_properties_typeProperties_server": {
						"value": "[parameters('Onpremlinkedservice_properties_typeProperties_server')]"
					},
					"Onpremlinkedservice_properties_typeProperties_database": {
						"value": "[parameters('Onpremlinkedservice_properties_typeProperties_database')]"
					},
					"Onpremlinkedservice_properties_typeProperties_userName": {
						"value": "[parameters('Onpremlinkedservice_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}