{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "data-factory-copying-data"
		},
		"datalinkedservice_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'datalinkedservice'"
		},
		"sqldatalinked_password": {
			"type": "secureString",
			"metadata": "Secure string for 'password' of 'sqldatalinked'"
		},
		"datalinkedservice_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://sdadfdata.dfs.core.windows.net/"
		},
		"sqldatalinked_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "project8server.database.windows.net"
		},
		"sqldatalinked_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "dbprojectadf"
		},
		"sqldatalinked_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "Ramon"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "data-factory-copying-data_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"datalinkedservice_accountKey": {
						"value": "[parameters('datalinkedservice_accountKey')]"
					},
					"sqldatalinked_password": {
						"value": "[parameters('sqldatalinked_password')]"
					},
					"datalinkedservice_properties_typeProperties_url": {
						"value": "[parameters('datalinkedservice_properties_typeProperties_url')]"
					},
					"sqldatalinked_properties_typeProperties_server": {
						"value": "[parameters('sqldatalinked_properties_typeProperties_server')]"
					},
					"sqldatalinked_properties_typeProperties_database": {
						"value": "[parameters('sqldatalinked_properties_typeProperties_database')]"
					},
					"sqldatalinked_properties_typeProperties_userName": {
						"value": "[parameters('sqldatalinked_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}